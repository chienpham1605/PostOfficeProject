
@model PostOffice.API.DTOs.User.UserBaseDTO
@{
    Layout = "~/Areas/Client/Views/Shared/_LayoutClient.cshtml";
}

<div class="breadcrumbs">
    <div class="col-sm-4">
        <div class="page-header float-left">
            <div class="page-title">
                <h1>Welcome back, @User.Identity.Name</h1>
            </div>
        </div>
    </div>
    <div class="col-sm-8">
        <div class="page-header float-right">
            <div class="page-title">
                <ol class="breadcrumb text-right">
                    <li class="active"></li>
                </ol>
            </div>
        </div>
    </div>
</div>

<div>
    @ViewData["UserId"]
</div>

<a asp-action="ParselCreate" asp-controller="Home">Create new order</a>
<a asp-action="MoneyOrder" asp-controller="Home">Create money Order</a>
